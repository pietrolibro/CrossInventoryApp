<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:CrossInventoryApp.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:dd="clr-namespace:CrossInventoryApp;assembly=CrossInventoryApp"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:CrossInventoryApp.Views"
        mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="450"
        d:DataContext="{x:Static dd:DesignData.MainWindowViewModel}"
        x:Class="CrossInventoryApp.Views.MainWindowView" Icon="/Assets/avalonia-logo.ico"
        Title="CrossInventoryApp" Content="{Binding Content}">


</Window>
